<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="uott.seg.mealerproject.activities.RegisterCookActivity">

    <include
        android:id="@+id/include"
        layout="@layout/activity_register"
        android:layout_width="420dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="-5dp"
        tools:layout_editor_absoluteY="-1dp" />

    <Button
        android:id="@+id/btnAddCook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="298dp"
        android:layout_marginTop="667dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="16dp"
        android:text="Regist"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvVoidCheck"
        android:layout_width="222dp"
        android:layout_height="37dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="499dp"
        android:layout_marginEnd="173dp"
        android:layout_marginBottom="195dp"
        android:text="Load a Void Check Here :"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <EditText
        android:id="@+id/teDesc"
        android:layout_width="340dp"
        android:layout_height="46dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="445dp"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="240dp"
        android:background="@android:drawable/alert_light_frame"
        android:ems="10"
        android:inputType="textPostalAddress|textMultiLine"
        android:scrollbarStyle="insideInset"
        android:scrollbars="horizontal|vertical"
        android:singleLine="false"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="349dp"
        android:layout_height="101dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="545dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="85dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <Button
        android:id="@+id/btnLoad"
        android:layout_width="98dp"
        android:layout_height="40dp"
        android:layout_marginStart="275dp"
        android:layout_marginTop="495dp"
        android:layout_marginEnd="38dp"
        android:layout_marginBottom="196dp"
        android:text="Load"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvVoidCheck"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvCookDesc"
        android:layout_width="140dp"
        android:layout_height="21dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="386dp"
        android:layout_marginEnd="248dp"
        android:layout_marginBottom="324dp"
        android:text="Cook Description :"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>